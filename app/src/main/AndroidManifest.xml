        </activity>
        <activity
            android:name=".DetailsActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="com.myfirstapp.shivamgupta.borrowcash.DetailsActivity" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity
            android:name=".GalleryActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="com.myfirstapp.shivamgupta.borrowcash.GalleryActivity" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
    </application>

</manifest>
