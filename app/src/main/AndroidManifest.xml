<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.myfirstapp.shivamgupta.borrowcash" >
    <uses-permission android:name="android.permission.INTERNET" />
    <application
        android:name="com.myfirstapp.shivamgupta.borrowcash.app.AppController"
        android:allowBackup="true"
        android:icon="@drawable/bb"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name=".SecondActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="com.myfirstapp.shivamgupta.borrowcash.SecondActivity" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>

        <activity
            android:name=".RegisterActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="com.myfirstapp.shivamgupta.borrowcash.RegisterActivity" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>

        <activity
            android:name=".LenderActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="com.myfirstapp.shivamgupta.borrowcash.LenderActivity" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>

        <activity
        android:name=".BorrowerActivity"
        android:label="@string/app_name" >
        <intent-filter>
            <action android:name="com.myfirstapp.shivamgupta.borrowcash.BorrowerActivity" />

            <category android:name="android.intent.category.DEFAULT" />
        </intent-filter>
        </activity>

        <activity
            android:name=".CalculatorActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="com.myfirstapp.shivamgupta.borrowcash.CalculatorActivity" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>

        <activity
            android:name=".LoginActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="com.myfirstapp.shivamgupta.borrowcash.LoginActivity" />

                <category android:name="android.intent.category.DEFAULT" />
